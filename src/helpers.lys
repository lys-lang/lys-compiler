// How do we define the abscense of a value?
struct Nil()

impl Nil {
  fun as(self: Nil): boolean = false
}

// repeats a string
fun repeat(str: string, times: i32): string = {
  var ret = ""
  var i = 0
  loop {
    if (i >= times) {
      break
    }
    ret = ret ++ str
    i = i + 1
    continue
  }
  ret
}